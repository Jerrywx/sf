package _75_颜色分类;

public class Sort {

	public static void main(String[] args) {
		

	}

	/// https://leetcode-cn.com/problems/sort-colors/
	public void sortColors(int[] nums) {
			
//		if (nums.length == 0) {return int[]}
		int l = 0;
		int r = nums.length - 1;
		int cur = r - 1;
		
		while (cur > l) {
			
			if (nums[cur] == 2) {
				nums[r--] = nums[cur--];
			} 
			else if (nums[cur] == 0) {
				nums[r++] = nums[cur--];
			} else {
				cur--;
			}
		}
    }
	
}
