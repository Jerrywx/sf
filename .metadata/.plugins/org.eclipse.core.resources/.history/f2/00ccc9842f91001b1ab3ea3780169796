package _88_排序数组;

public class Sourt {
	
	public void merge(int[] nums1, int m, int[] nums2, int n) {
		
		// nums1 = [1, 3, 5, 0, 0, 0] m = 3
		// nums2 = [2, 4, 6]		  n = 3
		
		int i1 = m - 1;
		int i2 = n - 1;
		int iCur = nums1.length - 1;
		
		while (i2 < iCur) {
			
			int num1 = nums1[i1];
			int num2 = nums2[i2];
			
			if (num1 > num2) {
				
				nums1[iCur] = num1;
				i1--;
				iCur--;
				
			} else {
				
			}
			
		}
		
	}

}
